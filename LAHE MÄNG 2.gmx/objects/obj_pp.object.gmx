//öö

if sc_night(){
    d_index = 1;
}

//liikumine ja col
if room != r_kaev_kinni{
    if !dmode{
            //vasak
        if keyboard_check(vk_left){
            if place_empty(x-pspeed,y+98){
                x -= pspeed;
            }
            xtrue = true;
            image_xscale = 1;
            //parem
        }else if keyboard_check(vk_right){
            if place_empty(x+pspeed,y+98){
                x += pspeed;
            }
            xtrue = true
            image_xscale = -1;
        }else{
            xtrue = false;
        }
            //üles
        if keyboard_check(vk_up){
            if place_empty(x,y-pspeed+98){
                y -= pspeed;
            }
            ys = 2;
            //alla
        }else if keyboard_check(vk_down){
            if place_empty(x,y+pspeed+98){
                y += pspeed;
            }
            ys = 1;
        }else{ ys = 0;}
    }

    //paigal
    if ys == 0 &amp;&amp; !xtrue{
        paigal = true;
    }else{
        paigal = false;
        //magamisasjad
        zzz = 0;
        alarm[0] = -1;
        magama = false;
        silmadkinni = false;
    }
    
    //col mask
    if !vaip{
        if xtrue || paigal{
            mask_index = spr_rula;
        }else{
            mask_index = spr_v_rula;
        }
    }else{
        if xtrue || paigal{
            mask_index = spr_vaip;
        }else{
            mask_index = spr_v_vaip;
        }
    }
    
    //col
    while image_xscale == -1 &amp;&amp; !place_empty(x+1, y+98) &amp;&amp; place_empty(x-27, y+98){
        x --;
    }
    while image_xscale == 1 &amp;&amp; !place_empty(x-1, y+98) &amp;&amp; place_empty(x+27, y+98){
        x ++;
    }

}else{//kinni
    if !dmode &amp;&amp; !controlkits{
        if keyboard_check(vk_left){
            image_xscale = 1;
            lookleft = true;
        }
        if keyboard_check(vk_right){
            image_xscale = -1;
            lookleft = false;
        }
    }
    sc_kitse_npc();
    
    /*//col mask
    if !vaip{
        if xtrue || paigal{
