        }
    }else{
        if xtrue || paigal{
            mask_index = spr_vaip;
        }else{
            mask_index = spr_v_vaip;
        }
    }*/
}

//pilgutamine
if pilk_aeg == 0{
    image_speed = 1;
    pilk_aeg = irandom(room_speed*14)+room_speed*1;
}
pilk_aeg --;
if image_speed == 1 &amp;&amp; anim != 0{
    anim --;
}else if anim == 0{
    anim = 5;
    image_speed = 0;
    image_index = d_index;
}

//magamine
if sc_night() &amp;&amp; keyboard_key == vk_nokey &amp;&amp; eivajuta_sec &amp;&amp; !dmode{
    if alarm[0] == -1{
        alarm[0] = room_speed*5;
    }
    silmadkinni = true;
    if magama{
        if unisuureneb{
            zzz += 0.01;
        }else{
            zzz -= 0.01;
        }
        if zzz &lt;= 0{//if zzz == 0{NÄIDE
            unisuureneb = true;
        }else if zzz == 1{
            unisuureneb = false;
        }
    }
}else{
    zzz = 0;
    alarm[0] = -1;
    magama = false;
    silmadkinni = false;
}

//magamine lisa
if keyboard_key == vk_nokey &amp;&amp; !eivajuta_eelmine{
    alarm[1] = room_speed*2;
}
eivajuta_eelmine = keyboard_key == vk_nokey;

if keyboard_key != vk_nokey{
    alarm[1] = -1;
    eivajuta_sec = false;
}
