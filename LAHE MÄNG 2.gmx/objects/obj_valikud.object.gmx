            <string>//edasiminek
if tuba != 7{
    if voib() &amp;&amp; mouse_check_button_released(mb_left) &amp;&amp; sees == true {
        tuba++;
        audio_play_sound(s_click,1,false);
    }
}else{
    if keyboard_check_released(vk_space) &amp;&amp; sees == true {
        tuba++;
        audio_play_sound(s_click,1,false);
    }
}

if tuba == 8{
    peatukk = 1;
    room_goto(r_peatukk);
}
//1
if tuba == 1
{
    //1(rula)
    dstate1 = point_in_rectangle(mouse_x, mouse_y, 1/3*900 -20, 300-10, 1/3*900 + 20, 300 + 10);
    //2(vaip)
    dstate2 = point_in_rectangle(mouse_x, mouse_y, 2/3*900 -20, 300-10, 2/3*900 + 20, 300 + 10);
    //logic
    if dstate1 || dstate2 {
        sees = true;
        if dstate1 == true {vaip = false;}
        else{vaip = true;}
    }else{sees = false;}
    dstate3 = false;
}
//2
if tuba == 2
{
    //1(hele)
    dstate1 = point_in_rectangle(mouse_x, mouse_y, 1/4*900 -20, 350-10, 1/4*900 + 20, 350 + 10);
    //2(tume)
    dstate2 = point_in_rectangle(mouse_x, mouse_y, 2/4*900 -20, 250-10, 2/4*900 + 20, 250 + 10);
    //3(täpp)
    dstate3 = point_in_rectangle(mouse_x, mouse_y, 3/4*900 -20, 350-10, 3/4*900 + 20, 350 + 10);
    //logic
    if dstate1 || dstate2 || dstate3 {
        sees = true;
        if dstate1 == true {sokk = 1;}
        if dstate2 == true {sokk = 2;}
        if dstate3 == true {sokk = 3;}
    }else{sees = false;}
}
//3
if tuba == 3
{
    //1(bedtime)
    dstate1 = point_in_rectangle(mouse_x, mouse_y, 1/4*900 -20, 300-10, 1/4*900 + 20, 300 + 10);
    if dstate1 = true{
        if mouse_wheel_down(){bedtime --;}
        if mouse_wheel_up(){bedtime ++;}
    }
    //2(ok)
    dstate2 = point_in_rectangle(mouse_x, mouse_y, 2/4*900 -20, 390-10, 2/4*900 + 20, 390 + 10);
    //3(uptime)
    dstate3 = point_in_rectangle(mouse_x, mouse_y, 3/4*900 -20, 300-10, 3/4*900 + 20, 300 + 10);
    if dstate3 = true{
        if mouse_wheel_down(){uptime --;}
        if mouse_wheel_up(){uptime ++;}
    }
    //logic
    if bedtime == 24 {bedtime = 0;}
    if bedtime == -1 {bedtime = 23;}
    if uptime == 24 {uptime = 0;}
    if uptime == -1 {uptime = 23;}
    
    if bedtime &gt; uptime{
        soovitus = 24 - (bedtime - uptime);
    }else{
        soovitus = uptime - bedtime;
    }
    if soovitus == 1 || 0 {h = " tund";}else {h = " tundi";}
    if soovitus &lt; 6{
        ai = 0;
        if dstate2{ 
            if kangekaelne &lt; 5 &amp;&amp; voib() &amp;&amp; mouse_check_button_released(mb_left) == true{
                kangekaelne++;
            }
        }else{
            kangekaelne = 0;  
        }
    }else{
        ai = 2;
        lahe = false;
        alarm[0] = -1;
        kk = false;
    }
    if kangekaelne == 5 {
        alarm[0] = room_speed*2;
        kk = true;    
    }
    if dstate2 == true &amp;&amp; (ai == 2 || lahe == true) {sees = true;}else{sees = false;}
}
//4
if tuba == 4{
    //okei
    dstate2 = point_in_rectangle(mouse_x, mouse_y, 1/2*900 -20, 300-10, 1/2*900 + 20, 300 + 10);
    //logic
    if dstate2 == true{sees = true;}else{sees = false;}
}
//5
if tuba == 5{
    //hüvasti
    dstate1 = point_in_rectangle(mouse_x, mouse_y, 1/4*900 -20, 300-10, 1/4*900 + 20, 300 + 10);
    //ei tahtnudki
    dstate2 = point_in_rectangle(mouse_x, mouse_y, 3/4*900 -20, 300-10, 3/4*900 + 20, 300 + 10);
    //logic
    if dstate1 || dstate2 {
        sees = true;
        if dstate1 == true{
            valu = false;
            aku = room_speed*3;
        }else{
            valu = true;
            aku = room_speed*10;
        }
    }else{sees = false;}
}
//6
if tuba == 6{
    sees = false;
    if aku == 0{tuba++;}else{aku--;}
    dstate2 = false;
    dstate3 = false;
}
//7
if tuba == 7{
    //alustame
    dstate1 = selected
    //logic
    if keyboard_check_released(vk_up) || keyboard_check_released(vk_down) || keyboard_check_released(vk_left) || keyboard_check_released(vk_right){
        if selected{
            selected = false;
        }else{
            selected = true;
        }
    }
    if dstate1{
        sees = true;
    }else{
        sees = false;
    }
}
//hover heli
if voib(){
    if (dstate1 || dstate2 || dstate3) != oldhover{
        audio_play_sound(s_click_h,1,false);
    }
    oldhover = (dstate1 || dstate2 || dstate3);
    
}
